    <!-- Core Stylesheet -->
    <link rel="stylesheet" href="~/Content/style.css">
    <link href="~/Content/style.css.map">
    <link href="~/Content/fontscss.css" rel="stylesheet" />
    @Scripts.Render("~/bundles/jquery")


</head>

<body>
    <!-- Preloader -->
